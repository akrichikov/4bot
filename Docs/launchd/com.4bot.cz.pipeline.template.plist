<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple Computer//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
  <key>Label</key>
  <string>com.4bot.cz.pipeline</string>
  <key>ProgramArguments</key>
  <array>
    <string>/usr/bin/env</string>
    <string>bash</string>
    <string>-lc</string>
    <string>cd ${REPO_ROOT} && . .venv/bin/activate && scripts/shell/launch_complete_pipeline.sh start</string>
  </array>
  <key>RunAtLoad</key>
  <true/>
  <key>KeepAlive</key>
  <true/>
  <key>StandardOutPath</key>
  <string>${LOG_DIR}/launchd_cz_pipeline.out</string>
  <key>StandardErrorPath</key>
  <string>${LOG_DIR}/launchd_cz_pipeline.err</string>
</dict>
</plist>
